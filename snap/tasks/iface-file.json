{
  "version": 1,
  "schedule": {
    "type": "simple",
    "interval": "360s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/procfs/iface/*": {}
      },
     "publish": [
        {
          "plugin_name": "elasticsearch",
          "config": {
            "protocol": "http",
            "address": "grafana",
            "port": 9200,
            "index": "snap-log",
            "publish_fields": "Namespace|Config|Data|Timestamp|Unit|Version"
          }
        }
      ]
     }
  }
}
