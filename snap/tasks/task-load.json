{
  "version": 1,
  "schedule": {
    "type": "simple",
    "interval": "120s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/procfs/load/*": {}
      },
      "config": {
        "/intel/procfs/load": {
          "proc_path": "/proc"
        }
      },
      "publish": [
        {
          "plugin_name": "elasticsearch",
          "config": {
            "protocol": "http",
            "address": "grafana",
            "port": 9200,
            "index": "snap-log",
            "publish_fields": "Namespace|Config|Data|Timestamp|Unit|Version"
          }
        }
      ]
    }
  }
}
